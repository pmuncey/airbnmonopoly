<div class="page-header">
  <h3>Bookmark #<%= @bookmark.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Investor</dt>
      <dd><%= @bookmark.investor_id %></dd>

      <dt>Property</dt>
      <dd><%= @bookmark.property_id %></dd>

      <dt>Pathway</dt>
      <dd><%= @bookmark.pathway_id %></dd>

      <dt>Joint venture</dt>
      <dd><%= @bookmark.joint_venture_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/bookmarks" class="btn btn-primary">
        Back
      </a>
      <a href="/bookmarks/<%= @bookmark.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_bookmark/<%= @bookmark.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
